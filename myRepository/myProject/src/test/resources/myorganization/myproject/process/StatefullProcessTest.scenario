<scenario>
  <name>StatefullProcessTest.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>all</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <fact-data>
      <type>MyObject</type>
      <fieldData>
        <field-data>
          <name>myID</name>
          <value>0</value>
          <nature>4</nature>
        </field-data>
      </fieldData>
      <name>$myObject</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>MY SECOND PROCESS RULE</ruleName>
      <expectedFire>true</expectedFire>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>myDescription</fieldName>
          <expected>AN OBJECT WITHOUT DESCRIPTION</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$myObject</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName>myorganization.myproject.process</packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>myorganization.myproject.bom.MyObject</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
  <ksessions>
    <string>kieSessionProcessStateful</string>
  </ksessions>
</scenario>